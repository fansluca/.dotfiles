{
  "layer": "top",
  "position": "top",
  "height": 40,
  "passthrough": false,
  "gtk-layer-shell": true,
  //"spacing": 4,

  "modules-left": [
    "hyprland/workspaces",
    "tray",
    "wlr/taskbar",
    "privacy",
    "custom/updates",
    "mpris"
  ],

  "modules-center": [
    //"mpd",
    "hyprland/window"
  ],

  "modules-right": [
    // "idle_inhibitor",
    //"group/one",
    "cpu",
    "memory",
    "disk",
    "temperature",
    //"group/two",
    "pulseaudio",
    "bluetooth",
    "backlight",
    //"group/three",
    "network",
    "battery",
    "clock"
  ],

  "hyprland/workspaces": {
    "disable-scroll": true,
    "on-click": "activate",
    "all-outputs": false,
    "format": "{name}",
    "on-scroll-up": "hyprctl dispatch workspace m-1 > /dev/null",
    "on-scroll-down": "hyprctl dispatch workspace m+1 > /dev/null",
    "format-icons": {
      "1": "ä¸€",
      "2": "äºŒ",
      "3": "ä¸‰",
      "4": "å››",
      "5": "äº”",
      "6": "å…­",
      "7": "ä¸ƒ",
      "8": "å…«",
      "9": "ä¹",
      "10": "å",
      "urgent": "ïª ",
      "focused": "ï†’ ",
      "default": "ï„‘ "
    }
  },
  "tray": {
    "icon-size": 25,
    "show-passive-items": true,
    "spacing": 10
  },

  "wlr/taskbar": {
    "format": "{icon}",
    "icon-size": 18,
    "tooltip-format": "{title}",
    "on-click": "activate",
    "on-click-middle": "close",
    "ignore-list": ["kitty"],
    "app_ids-mapping": {
      "firefoxdeveloperedition": "firefox-developer-edition"
    },
    "rewrite": {
      "Firefox Web Browser": "Firefox",
      "Foot Server": "Terminal"
    }
  },

  "hyprland/window": {
    "max-length": 50,
    "format": "{}",
    "rewrite": {
      //"google-chrome": "<span color=\"#4285F4\">ï‰¨ </span>  Chrome",
      //"firefox": "<span color=\"#E86029\">ï‰© </span>  Firefox",
      //"kitty": "<span color=\"#6F4020\">ï’‰ </span>  Kitty",
      //"org.telegram.desktop": "<span color=\"#2694CD\">ï‹† </span>  Telgram",
      "": "<span color=\"#049BE1\">\uf359</span> Hyprland Desktop",
      "(.*) â€” Mozilla Firefox": "<span color=\"#E86029\">ï‰© </span> $1",
      "(.*) - Google Chrome": "<span color=\"#4285F4\">ï‰¨ </span> $1",
      "Telegram (.*)": "<span color=\"#2694CD\">ï‹† </span> Telegram",
      "(.*)~": "<span color=\"#6F4020\">ï’‰ </span> $1"
    },
    "separate-outputs": true
  },

  "mpd": {
    "format": "{stateIcon} {consumeIcon}{randomIcon}{repeatIcon}{singleIcon}{artist} - {album} - {title} ({elapsedTime:%M:%S}/{totalTime:%M:%S}) â¸¨{songPosition}|{queueLength}â¸© {volume}% ï€ ",
    "format-disconnected": "Disconnected ï€ ",
    "format-stopped": "{consumeIcon}{randomIcon}{repeatIcon}{singleIcon}Stopped ï€ ",
    "unknown-tag": "N/A",
    "interval": 2,
    "consume-icons": {
      "on": "ïƒ„ "
    },
    "random-icons": {
      "off": "<span color=\"#f53c3c\">ï´</span> ",
      "on": "ï´ "
    },
    "repeat-icons": {
      "on": "ï€ "
    },
    "single-icons": {
      "on": "ï€1 "
    },
    "state-icons": {
      "paused": "ïŒ",
      "playing": "ï‹"
    },
    "tooltip-format": "MPD (connected)",
    "tooltip-format-disconnected": "MPD (disconnected)"
  },
  "mpris": {
    "dynamic-len": 32,
    "format": "{player_icon} {dynamic}",
    "on-click-middle": "playerctl play-pause",
    "on-click": "playerctl previous",
    "on-click-right": "playerctl next",
    "format-paused": "{status_icon} <i>{dynamic}</i>",
    "player-icons": {
      "default": "â–¶",
      "mpv": "ğŸµ"
    },
    "status-icons": {
      "paused": "â¸"
    }
  },

  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ó°› ",
      "deactivated": "ó°›‘"
    },
    "tooltip": true
  },
  "keyboard-state": {
    "numlock": true,
    "capslock": true,
    "format": "{name} {icon} ",
    "format-icons": {
      "locked": "ï€£",
      "unlocked": "ï‚œ "
    }
  },

  "group/one": {
    "orientation": "inherit",
    "modules": ["cpu", "memory", "disk", "temperature"]
  },
  "cpu": {
    "interval": 10,
    "format-alt": "ï‹›",
    "format": "{usage}% ï‹›",
    "format-alt-click": "click-right",
    "on-click": "kitty --title btop sh -c 'btop'",
    "states": {
      "warning": 70,
      "critical": 90
    }
  },
  "memory": {
    "interval": 30,
    "format-alt": "ó°›",
    "format": "{}% ó°›",
    "format-alt-click": "click-right",
    "on-click": "kitty --title btop sh -c 'btop'",
    "states": {
      "warning": 70,
      "critical": 90
    }
  },
  "disk": {
    "interval": 30,
    "format-alt": "ï‚ ",
    "format": "{percentage_used}% ï‚ ",
    "format-alt-click": "click-right",
    "on-click": "kitty --title btop sh -c 'btop'",
    "path": "/",
    "states": {
      "warning": 70,
      "critical": 90
    }
  },
  "temperature": {
    //"hwmon-path": "/sys/class/hwmon/hwmon4/temp1_input",
    "interval": 5,
    "format-alt": "{icon}",
    "format": "{temperatureC}Â°C {icon}",
    "format-alt-click": "click-right",
    "format-icons": ["ï‹‹", "ï‹Š", "ï‹‰", "ï‹ˆ", "ï‹‡"],
    "critical-threshold": 80,
    "on-click": "kitty --title btop sh -c 'btop'"
  },

  "group/two": {
    "orientation": "inherit",
    "modules": ["pulseaudio", "bluetooth", "backlight"]
  },
  "pulseaudio": {
    "scroll-step": 5,
    "max-volume": 100,
    "on-click-middle": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
    "format": "{volume}% {icon} {format_source}",
    "format-bluetooth": "{volume}% {icon}ïŠ” {format_source}",
    "format-bluetooth-muted": "ó°Ÿ {icon}ïŠ” {format_source}",
    "format-muted": "MUTE ó°Ÿ {format_source}",
    "format-source": "{volume}% ï„°",
    "format-source-muted": "ï„± ",
    "format-icons": {
      "headphone": "ï€¥ ",
      "hands-free": "ï– ",
      "headset": "ó°‹ ",
      "phone": "ï‚• ",
      "portable": "ï‚• ",
      "car": "ï†¹ ",
      "default": ["ï€§", "ï€¨ "]
    },
    "on-click": "pavucontrol",
    "ignored-sinks": ["Easy Effects Sink"]
  },
  "pulseaudio#microphone": {
    "format": "{format_source}",
    "format-source": "ï¡« {volume}%",
    "format-source-muted": "ï¡¬ Muted",
    "on-click": "pamixer --default-source -t",
    "on-scroll-up": "pamixer --default-source -i 5",
    "on-scroll-down": "pamixer --default-source -d 5",
    "scroll-step": 5
  },
  "backlight": {
    // "device": "acpi_video1",
    "format": "{percent}% {icon}",
    "format-icons": ["î", "î“", "î‘", "î", "î", "î", "îŠ", "îˆ", "î›"]
  },

  "group/three": {
    "orientation": "inherit",
    "modules": ["network", "battery"]
  },
  "network": {
    //"interface": "wlan0", // (Optional) To force the use of this interface
    "interval": 5,
    "format-wifi": "{essid} ï‡« ",
    "format-ethernet": "{ipaddr}/{cidr} ï– ",
    "on-click": "~/.config/rofi/wifi/wifi.sh",
    "on-click-right": "~/.config/rofi/wifi/wifinew.sh",
    "tooltip-format": "{ifname} via {gwaddr} ï– ",
    "format-linked": "{ifname} (No IP) ï– ",
    "format-disconnected": "Disconnected âš ",
    "format-alt": "ïƒ˜{bandwidthUpBits} ïƒ—{bandwidthDownBits}"
  },
  "bluetooth": {
    "format": "{status}ïŠ”",
    "format-disabled": "", // an empty format will hide the module
    "format-connected": "{num_connections} ïŠ”",
    //"on-click": "~/.config/waybar/scripts/bluetooth/type-1/blt-connect.sh",
    "tooltip-format": "{device_alias}",
    "tooltip-format-connected": "{device_enumerate} ïŠ”",
    "tooltip-format-enumerate-connected": "{device_alias}"
  },
  "battery": {
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": "{capacity}% {icon}",
    "format-charging": "{capacity}% ğŸ—² ",
    "format-plugged": "{capacity}% ï‡¦ ",
    "format-alt": "{time} {icon}",
    "format-icons": ["ï‰„ ", "ï‰ƒ ", "ï‰‚ ", "ï‰ ", "ï‰€ "]
  },
  "battery#bat2": {
    "bat": "BAT2"
  },

  "clock": {
    "format": "<span color='#bf616a'>ï€— </span> {:%H:%M}",
    "format-alt": "<span color='#bf616a'>ï³ </span> {:%a %b %d}",
    "tooltip-format": "<big>{:%B %Y}</big>\n<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 3,
      "weeks-pos": "right",
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "days": "<span color='#ecc6d9'><b>{}</b></span>",
        "weeks": "<span color='#99ffdd'><b>W{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b><u>{}</u></b></span>"
      }
    },
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down"
    }
  },

  "custom/updates": {
    "format": "ï€™ {}",
    "tooltip-format": "{}",
    "escape": true,
    "return-type": "json",
    "exec": "~/.config/waybar/scripts/updates.sh",
    "on-click": "~/.config/waybar/scripts/install_updates.sh",
    "restart-interval": 60,
    "single": 8,
    "tooltip": true
  },

  "privacy": {
    "icon-spacing": 4,
    "icon-size": 18,
    "transition-duration": 250,
    "modules": [
      {
        "type": "screenshare",
        "tooltip": true,
        "tooltip-icon-size": 24
      },
      {
        "type": "audio-out",
        "tooltip": true,
        "tooltip-icon-size": 24
      },
      {
        "type": "audio-in",
        "tooltip": true,
        "tooltip-icon-size": 24
      }
    ]
  }
}
